{
  "name": "@takamiya1021/aoi-terminals",
  "version": "0.1.0",
  "description": "WSL向けWebベースターミナル（tmux + Share QR）",
  "directories": {
    "doc": "doc"
  },
  "files": [
    "backend/config/",
    "backend/src/",
    "backend/package.json",
    "backend/package-lock.json",
    "backend/tsconfig.json",
    "backend/jest.config.cjs",
    "frontend/public/",
    "frontend/src/",
    "frontend/package.json",
    "frontend/package-lock.json",
    "frontend/next.config.mjs",
    "frontend/postcss.config.mjs",
    "frontend/tailwind.config.ts",
    "frontend/tsconfig.json",
    "README.md"
  ],
  "scripts": {
    "setup": "npm --prefix backend ci && npm --prefix frontend ci",
    "start": "./scripts/start.sh",
    "stop": "./scripts/stop.sh",
    "test": "npm run test:frontend && npm run test:backend && npm run test:e2e",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "test:e2e": "cd frontend && npx playwright test",
    "test:e2e:existing": "cd frontend && npx playwright test --config playwright.config.existing.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "qrcode-terminal": "^0.12.0"
  }
}
